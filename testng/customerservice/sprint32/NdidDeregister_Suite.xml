    <test name="iOS NDID Deregister">
        <parameter name="DeviceProfile" value="customer-service-ios-oneapp-sim-iphone12-pro-max-ndid-saucelabs"/>
        <groups>
            <run>
                <include name="Activate new flow"/>
                <include name="Enter Service Screen Dev Latest" />
                <include name="Customer Service NDID list Deregister"/>
                <include name="Customer Service NDID Service Detail"/>
                <include name="Customer Service deregister biometric consent in case click discard"/>
                <include name="Customer Service deregister biometric consent in case click deregister"/>
                <include name="Customer Service deregister successful"/>
                <include name="Customer Service is not register NDID"/>
            </run>
        </groups>
        <classes>
            <class name="com.ttbbank.oneapp.runner.customerservice.sprint32.NdidDeregisterRunner"/>
        </classes>
    </test>

