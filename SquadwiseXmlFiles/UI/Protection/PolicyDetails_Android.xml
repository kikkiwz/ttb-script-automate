			</run>
		</groups>
		<classes>
			<class name="com.ttbbank.oneapp.runner.protection.PolicyDetailsRunner"/>
		</classes>
	</test>

	<test name="Protection: Policy Details Customer6">
		<parameter name="DeviceProfile" value="protection-android-customer06"/>
