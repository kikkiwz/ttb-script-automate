			</run>
		</groups>
		<classes>
			<class name="com.ttbbank.oneapp.runner.lending.LendingIOSRunner"/>
		</classes>
	</test>
	<test name="Lending_iOS_PRODUCT_LANDING_BPlus">
		<parameter name="DeviceProfile" value="lending_ios_saucelabs"/>
